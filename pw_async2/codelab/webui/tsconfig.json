{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "lib": [
      "ES2021"
    ],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "www/main.ts"
  ]
}
